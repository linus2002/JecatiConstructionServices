    <script>
        document.addEventListener('DOMContentLoaded', (event) => {
            const popup = document.getElementsByClassName('popup')[0]; // Get the first popup element
            const submitInquiryButton = document.getElementsByClassName('submit-inquiry')[0]; // Get the first submit inquiry button
            const jecati = document.getElementsByClassName('jecati')[0]; // Get the first jecati button
            const closeBtn = document.getElementById('closeBtn');
