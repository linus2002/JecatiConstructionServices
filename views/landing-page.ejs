            const gmail = document.getElementById('gmail');
            const fullname = document.getElementById('fullNameInput'); // Fix: changed 'gmail' to 'fullname'
            const contactNumber = document.getElementById('contact'); // Fix: changed 'gmail' to 'contactNumber'
            const services = document.getElementById('services'); // Fix: changed 'gmail' to 'services'
            const startOfSchedule = document.getElementById('startOfSchedule'); // Fix: changed 'gmail' to 'startOfSchedule'
            const endOfSchedule = document.getElementById('endOfSchedule'); // Fix: changed 'gmail' to 'endOfSchedule'
